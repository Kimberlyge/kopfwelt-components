
{{#if og.type}}
	<meta property="og:type" content="{{og.type}}" />
{{/if}}

{{#if og.metaTitle}}
	<meta property="og:title" content="{{og.metaTitle}}" />
{{/if}}

{{#if og.metaDescription}}
	<meta property="og:description" content="{{og.metaDescription}}" />
{{/if}}

{{#if og.url}}
	<meta property="og:url" content="{{og.url}}" />
{{/if}}

{{#if og.metaUrl}}
	<meta property="og:image" content="{{og.metaUrl}}" />
{{/if}}

{{#if og.metaImage}}
	<meta property="og:image" content="{{og.metaImage}}" />
{{/if}}
